{
  "expo": {
    "name": "PokemonFun",
    "slug": "PokemonFun",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/app.png",
    "loading": {
      "icon": "./assets/icons/loading.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Followed the tutorial by tscans https://github.com/tscans/PokeSearch.  Only publishing to show progress",
    "githubUrl": "https://github.com/KellyDeLaVergne/PokemonFun"
  }
}
